interface Musician {
    void playInstrument();
}

interface Athlete {
    void playSport();
}

class StudentLeader implements Musician, Athlete {
    public void playInstrument() {
        System.out.println("Plays guitar");
    }

    public void playSport() {
        System.out.println("Plays soccer");
    }
}

public class MultipleInheritance {
    public static void main(String[] args) {
        StudentLeader victor = new StudentLeader();
        victor.playInstrument();
        victor.playSport();
    }
}

